<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="400.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cl.gz.lockapp.controller.PanelMensajeController">
    <stylesheets>
        <URL value="@/cl/gz/lockapp/css/panelmensaje.css" />
    </stylesheets>
   <children>
      <VBox prefHeight="200.0" prefWidth="400.0">
         <children>
            <HBox prefHeight="195.0" prefWidth="400.0">
               <children>
                  <TextArea fx:id="mensajeRecivido" editable="false" prefHeight="195.0" prefWidth="400.0" wrapText="true" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onMouseClicked="#btnOkEvent" text="ACEPTAR">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
